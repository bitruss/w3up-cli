{
  "name": "@web3-storage/w3up-cli",
  "description": "w3up interface",
  "version": "0.6.0",
  "main": "./src/main.js",
  "bin": {
    "w3up": "./src/cli.js"
  },
  "keywords": [
    "w3up"
  ],
  "files": [
    "src"
  ],
  "scripts": {
    "build": "npm run build:api_docs && npm run build:types",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir ./types",
    "build:api_docs": "jsdoc2md -f src/index.js > API.md",
    "pretest": "npm run prettier && npm run typecheck",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier -c '{src,test}/**/*.{js,ts,yml,json}' --ignore-path .gitignore",
    "prettier:fix": "prettier --write '{src,test}/**/*.{js,ts,yml,json}' --ignore-path .gitignore",
    "test:coverage": "vitest run --coverage",
    "test:dev": "vitest"
  },
  "dependencies": {
    "@ipld/unixfs": "^1.1.0-dev",
    "@ucanto/server": "^0.6.4",
    "@web-std/stream": "^1.0.1",
    "archy": "^1.0.0",
    "conf": "^10.2.0",
    "dotenv": "^16.0.1",
    "inquirer": "^9.1.0",
    "ora": "^6.1.2",
    "uuid": "^8.3.2",
    "w3up-client": "web3-storage/w3up-client",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.1",
    "@types/yargs": "^17.0.11",
    "chai": "^4.3.6",
    "chai-subset": "^1.6.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vitest": "^0.23.2",
    "semantic-release": "^19.0.5"
  },
  "exports": {
    ".": {
      "import": "./src/lib.js"
    }
  },
  "release": {
    "branches": ["master", "next"]
  },
  "type": "module",
  "license": "(Apache-2.0 AND MIT)"
}
